<div class="element-content">
    <div class="row">
        <div class="col-sm-2 col-xxxl-2">
            <a class="element-box el-tablo" href="#">
                <div class="label">
                    Sim Actif
                </div>
                <div class="value">
                    {{ simCardStat.activeSimCard }}
                </div>
                <div class="trending">
                    <span>100%</span>
                </div>
            </a>
        </div>
        <div class="col-sm-2 col-xxxl-2">
            <a class="element-box el-tablo" href="#">
                <div class="label">
                    Nbre de PDV
                </div>
                <div class="value">
                    {{ simCardStat.activePointofsale }}
                </div>
                <div class="trending">
                    <span>{{ ((simCardStat.activePointofsale / simCardStat.activeSimCard) *100) | round(1, 'floor') }}%</span>
                </div>
            </a>
        </div>
        <div class="col-sm-2 col-xxxl-2">
            <a class="element-box el-tablo" href="#">
                <div class="label">
                    Nbre de AGNT
                </div>
                <div class="value">
                    {{ simCardStat.activePointofsaleByProfile('AGNT') }}
                </div>
                <div class="trending">
                    <span>{{ ((simCardStat.activePointofsaleByProfile('AGNT') / simCardStat.activeSimCard) *100) | round(1, 'floor') }}%</span>
                </div>
            </a>
        </div>
        <div class="col-sm-2 col-xxxl-2">
            <a class="element-box el-tablo" href="#">
                <div class="label">
                    Nbre de DISTRO
                </div>
